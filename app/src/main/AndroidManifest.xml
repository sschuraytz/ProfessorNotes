<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.macbook.professor_notes">

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher_professor_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_professor_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".RecordListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecordPagerActivity"
            android:parentActivityName=".RecordListActivity">
        </activity>
    </application>

</manifest>